<div class="row">
  <h1>Action</h1>
  <table>
    <tr>
      <th>Type</th>
      <th>Price</th>
      <th>When</th>
      <th>Ago</th>
    </tr>
    {{#each actionsArray}}
      <tr>
        <td>{{type}}</td>
        <td>{{cost}}</td>
        <td>{{time}}</td>
        <td>{{ago}}</td>
      </tr>
    {{/each}}
  </table>
</div>
